<div class="img-container">
    <img class="modal__img" src="{{images.8.url}}" alt="" />
</div>
<div class="img-container__big">
    <img class="img-modal_big" src="{{images.2.url}}" alt="team__twelve_" />
</div>
<ul class="modal__list">

    {{#if info}}
    <li class="modal__item">
        <h3 class="modal__title">INFO</h3>
        <p class="modal__text modal__text-info">{{info}}</p>
    </li>
    {{/if}}
    {{#if dates.start}}
    <li class="modal__item">
        <h3 class="modal__title">WHEN</h3>
        <ul class="content__list">
            <li class="modal__text">{{dates.start.localDate}}</li>
            <li class="modal__text">{{dates.start.localTime}}</li>
        </ul>
    </li>
    {{/if}}
    {{#if _embedded.venues}}
    <li class="modal__item">
        <h3 class="modal__title">WHERE</h3>
        <ul class="content__list">
            <li class="modal__text">{{_embedded.venues.0.city.name}}, {{_embedded.venues.0.country.name}}</li>
            <li class="modal__text">{{_embedded.venues.0.name}}</li>
        </ul>        
    </li>
    {{/if}}

    {{#if name}}
    <li class="modal__item">
        <h3 class="modal__title">WHO</h3>
        <span class="modal__text">{{name}}</span>
    </li>
    {{/if}}
    {{#if priceRanges}}
    <li class="modal__item">
        <h3 class="modal__title">PRICE</h3>
    {{/if}}        
